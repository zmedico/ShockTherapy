{
	"manifest_version": 2,
	"name": "Shock Therapy",
	"version": "1.0.0.12",
	"description": "Plasma globe simulator",
	"icons": {
		"16": "icons/High_voltage_warning_16.png",
		"48": "icons/High_voltage_warning_48.png",
		"128": "icons/High_voltage_warning_128.png"
	},
	"minimum_chrome_version": "23",
	"offline_enabled": true,
	"app": {
		"background": {
			"scripts": ["js/eventPage.js"],
			"persistent": false
		}
	},
	"permissions": [
		"storage"
	]
}
